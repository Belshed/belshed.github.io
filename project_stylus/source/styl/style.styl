

@font-face {
    font-family: 'Mplus 1p';
    src: url('../fonts/Mplus1p-Medium.woff2') format('woff2'),
        url('../fonts/Mplus1p-Medium.woff') format('woff');
    font-weight: 500;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: 'Mplus 1p';
    src: url('../fonts/Mplus1p-Regular.woff2') format('woff2'),
        url('../fonts/Mplus1p-Regular.woff') format('woff');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: 'Mplus 1p';
    src: url('../fonts/Mplus1p-Light.woff2') format('woff2'),
        url('../fonts/Mplus1p-Light.woff') format('woff');
    font-weight: 300;
    font-style: normal;
    font-display: swap;
}


@font-face {
    font-family: 'Old Standard TT';
    src: url('../fonts/OldStandardTT-Regular.woff2') format('woff2'),
        url('../fonts/OldStandardTT-Regular.woff') format('woff');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}


@font-face {
  font-family: 'icomoon';
  src:  url('../fonts/icons/icomoon.eot?x6233m');
  src:  url('../fonts/icons/icomoon.eot?x6233m#iefix') format('embedded-opentype'),
    url('../fonts/icons/icomoon.ttf?x6233m') format('truetype'),
    url('../fonts/icons/icomoon.woff?x6233m') format('woff'),
    url('../fonts/icons/icomoon.svg?x6233m#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}








/* GLOBAL
^^^^^^^^^^^^ */
after_clear()
	&::after
		display block
		content ''
		clear both
TransitionEase()
	transition all 0.3s ease
TransitionLinear()
	transition all 0.3s linear

MarginLastBottom()
	> *:last-child
		margin-bottom 0

MaxWidth()
	max-width 120rem
	margin 0 auto
	

TextColor = #232323
DarkBlueColor = #002941
BeigeColor = #D29966
Font = 'Old Standard TT', "Times New Roman", Times, Baskerville, Georgia, serif


html 
	height 100%
	font-size 62.5%

body 
